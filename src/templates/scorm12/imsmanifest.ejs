<?xml version="1.0" encoding="UTF-8"?>
<manifest identifier="<%= spec.identifier %>" version="1.0" xmlns="http://www.imsproject.org/xsd/imscp_rootv1p1p2" xmlns:adlcp="http://www.adlnet.org/xsd/adlcp_rootv1p2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.imsproject.org/xsd/imscp_rootv1p1p2 imscp_rootv1p1p2.xsd http://www.imsglobal.org/xsd/imsmd_rootv1p2p1 imsmd_rootv1p2p1.xsd http://www.adlnet.org/xsd/adlcp_rootv1p2 adlcp_rootv1p2.xsd">

  <metadata>
    <schema>ADL SCORM</schema>
    <schemaversion>1.2</schemaversion>
    <% if (spec.metadata) { %>
      <% if (spec.metadata.description) { %><adlcp:location><%= spec.metadata.description %></adlcp:location><% } %>
    <% } %>
  </metadata>

  <organizations default="<%= spec.identifier %>_org">
    <organization identifier="<%= spec.identifier %>_org">
      <title><%= spec.title %></title>
      <item identifier="<%= spec.identifier %>_item" identifierref="<%= spec.identifier %>_resource">
        <title><%= spec.title %></title>
        <adlcp:maxtimeallowed>PT0H30M0S</adlcp:maxtimeallowed>
        <adlcp:timelimitaction>continue,no message</adlcp:timelimitaction>
        <adlcp:datafromlms></adlcp:datafromlms>
        <adlcp:masteryscore><%= spec.masteryScore || 80 %></adlcp:masteryscore>
      </item>
    </organization>
  </organizations>

  <resources>
    <resource identifier="<%= spec.identifier %>_resource" type="webcontent" adlcp:scormtype="sco" href="<%= spec.launch || 'index.html' %>">
      <file href="<%= spec.launch || 'index.html' %>" />
      <% if (spec.objectType === 'code-practice') { %>
        <file href="styles/reset.css" />
        <file href="styles/site.css" />
        <file href="styles/panel.css" />
        <file href="scripts/panel.js" />
        <file href="scripts/editor.js" />
      <% } %>
      <dependency identifierref="common_files" />
    </resource>
    <resource identifier="common_files" type="webcontent" adlcp:scormtype="asset">
      <!-- Common SCORM files -->
    </resource>
  </resources>

</manifest>